{
  "name": "makercam5",
  "version": "1.0.0",
  "description": "Port of makercam written in flash to HTML5 using PIXI",
  "Note1": "Taken from: https://medium.com/dev-bits/everything-i-know-about-writing-modular-javascript-applications-37c125d8eddf",
  "Note2": "Taken from: https://medium.com/@carmenwick/webpack-tutorial-part-1-the-basics-f82e3879a3f8",
  "main": "./dist/makercam5.js",
  "scripts": {
    "start": "webpack-dev-server --color",
    "postinstall": "echo npm run bootstrap",
    "clean": "rimraf js/* dist/makercam5.*",
    "xpretest": "echo npm run lint && npm run build",
    "pretest": "npm run dev",
    "unit-test": "echo floss --path test",
    "coverage": "echo npm run pretest && nyc floss --path=test && nyc report --reporter=html",
    "coverage:ci": "echo npm run pretest && nyc floss --path=test && nyc report --reporter=text-lcov > coverage.lcov",
    "docs": "mkdirp dist && jsdoc -c jsdoc.conf.json -R README.md",
    "types": "echo npm run types:legacy && npm run types:default",
    "types:default": "echo mkdirp dist/types && jsdoc -c types/jsdoc.conf.json && node types/assemble pixi.js",
    "types:legacy": "echo mkdirp dist/types && jsdoc -c types/jsdoc-legacy.conf.json && node types/assemble pixi.js-legacy",
    "check-types": "tsc",
    "lint": "echo eslint *.js test bundles packages tools --ignore-path .gitignore --max-warnings 0",
    "lintfix": "echo npm run lint -- --fix",
    "dev": "webpack --env.exclude=false --mode development",
    "test": "mochapack --mode development --require test/pixi-setup.js test/partKart/*  ",
    "prebuild": "npm run clean",
    "build": "webpack --env.exclude=true --mode production",
    "postbuild": "echo npm run types",
    "watch": "echo rollup -cw",
    "predist": "nom run clean && npm run build && npm run types",
    "dist": "echo npm run docs",
    "postdist": "echo copyfiles -f bundles/*/dist/* dist && copyfiles -f \"packages/**/dist/*\" dist/packages",
    "prerelease": "echo npm run clean:build && npm test",
    "postversion": "echo npm run build:prod",
    "release": "echo npm publish . "
  },
  "xpre-commit": [
    "lintfix"
  ],
  "author": "John Talbot",
  "license": "MIT",
  "dependencies": {
    "jquery": "^3.4.1",
    "pixi.js": "^5.1.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.0",
    "@babel/core": "^7.7.2",
    "@babel/plugin-proposal-class-properties": "^7.1.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.6.2",
    "@babel/plugin-proposal-private-methods": "^7.1.6",
    "@babel/preset-env": "^7.7.1",
    "@babel/preset-typescript": "^7.7.2",
    "@pixi/jsdoc-template": "^2.4.2",
    "@types/jquery": "^3.3.31",
    "@types/mocha": "^5.2.7",
    "@types/underscore": "^1.9.4",
    "babel-loader": "^8.0.6",
    "canvas": "^2.6.0",
    "chai": "~4.0.0",
    "copyfiles": "^2.1.0",
    "eslint": "^5.16.0",
    "glob": "^7.1.3",
    "jsdoc": "^3.5.5",
    "jsdom-canvas-2": "^11.6.3-alpha.0",
    "minimist": "^1.2.0",
    "mocha": "^6.2.2",
    "mochapack": "^1.1.8",
    "nyc": "^14.1.1",
    "path": "^0.12.7",
    "pre-commit": "^1.2.2",
    "rimraf": "^2.6.3",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.5.2",
    "tsd-jsdoc": "^2.1.3",
    "typescript": "^3.7.2",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "underscore": "^1.9.1",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0"
  }
}
